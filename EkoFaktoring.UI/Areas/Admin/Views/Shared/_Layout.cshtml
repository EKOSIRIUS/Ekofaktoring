<!doctype html>
<html lang="tr">
  <head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover"/>
    <meta http-equiv="X-UA-Compatible" content="ie=edge"/>
    <title>Dashboard - Eko Faktoring</title>

    <!-- CSS-->
    <link href="~/dashboard/dist/css/tabler.min.css?1684106062" rel="stylesheet"/>
    <link href="~/dashboard/dist/css/tabler-flags.min.css?1684106062" rel="stylesheet" />
    <link href="~/dashboard/dist/css/tabler-payments.min.css?1684106062" rel="stylesheet" />
    <link href="~/dashboard/dist/css/tabler-vendors.min.css?1684106062" rel="stylesheet" />
    <link href="~/dashboard/dist/css/demo.min.css?1684106062" rel="stylesheet" />
    <style>
      @@import url('https://rsms.me/inter/inter.css');
      :root {
      	--tblr-font-sans-serif: 'Inter Var', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif;
      }
      body {
      	font-feature-settings: "cv03", "cv04", "cv11";
      }
    </style>
  </head>
  <body >
    <script src="~/dashboard/dist/js/demo-theme.min.js?1684106062"></script>
    <div class="page">
        @await Html.PartialAsync("_adminNavbarPartial")


        <div class="page-wrapper">
        <!-- Page header -->
            @await Html.PartialAsync("_adminHeaderPartial")

        
        <!-- Page body -->
        <div class="page-body">
                <div class="container-xl">
                    @RenderBody()
                </div>
        </div>
        @await Html.PartialAsync("_adminFooterPartial")
      </div>
    </div>
    @await Html.PartialAsync("_adminScriptsPartial")
    @RenderSection("_tinyMce",false)
  </body>
</html>