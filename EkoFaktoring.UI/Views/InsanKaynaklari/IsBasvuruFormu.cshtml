<section class="main-contact-form-area pdb100">
    <div class="container">
        <div class="row d-flex justify-content-center">
            <h1 style="margin-bottom: 150px" class=" text-center"><b>İş Başvuru Formu</b></h1>
            <div class="mt-5">
                <form itemid="jobform" class="d-flex flex-column align-items-center">
                    <div class="col-12 col-md-6">
                        <h2 style="color:green; text-align:center;">Kişisel Bilgiler</h2>
                    </div>
                    <div class="col-12 col-md-6 mb-3">
                        <label class="form-label text-center">Ad Soyad</label>
                        <input type="text" class="form-control" name="example-text-input" placeholder="Ad Soyad Giriniz.">
                    </div>

                    <div class="col-12 col-md-6 mb-3">
                        <label class="form-label text-center">Doğum Tarihi</label>
                        <input type="text" name="input-mask" class="form-control" data-mask="00/00/0000" data-mask-visible="true" placeholder="00/00/0000" autocomplete="off" />
                    </div>

                    <div class="col-12 col-md-6 mb-3">
                        <label class="form-label text-center">Doğum Yeri</label>
                        <input type="text" class="form-control" name="example-text-input" placeholder="Doğum Yeri Giriniz.">
                    </div>

                    <div class="col-12 col-md-6 mb-3">
                        <label class="form-label text-center">T.C Kimlik No</label>
                        <input type="number" class="form-control" name="example-text-input" placeholder="T.C Kimlik No Giriniz.">
                    </div>

                    <div class="col-12 col-md-6 mb-3 d-flex flex-row ">
                        <div class="col-4 d-flex flex-column justify-content-between">
                            <label class="form-label text-center">Cinsiyet</label>
                            <select class="">
                                <option value="1">Erkek</option>
                                <option value="2">Kadın</option>
                            </select>
                        </div>
                        <div class="col-4 d-flex flex-column justify-content-between">
                            <label class="form-label text-center">Medeni Hal</label>
                            <select class="">
                                <option value="1">Bekar</option>
                                <option value="2">Evli</option>
                                <option value="3">Dul</option>
                            </select>
                        </div>
                        <div class="col-4 d-flex flex-column justify-content-between">
                            <label class="form-label text-center">Askerlik Durumu</label>
                            <select class="">
                                <option value="1">Yaptı</option>
                                <option value="2">Yapmadı</option>
                            </select>
                        </div>
                    </div>

                    <div class="col-12 col-md-6 mb-3">
                        <label class="form-label text-center">SSK Sicil No</label>
                        <input type="number" class="form-control" name="example-text-input" placeholder="SSK Sicil No Giriniz.">
                    </div>

                    <div class="col-12 col-md-6 mb-3">
                        <label class="form-label text-center">Telefon Numarası</label>
                        <input type="text" name="input-mask" class="form-control" data-mask="(000) 000 0000" data-mask-visible="true" placeholder="(000) 000 0000" autocomplete="off" />
                    </div>

                    <div class="col-12 col-md-6 mb-3">
                        <label class="form-label text-center">E-Posta Adresi</label>
                        <input type="email" class="form-control" name="example-text-input" placeholder="E-Posta Adresi Giriniz.">
                    </div>

                    <div class="col-12 col-md-6 mb-3">
                        <label class="form-label text-center">Ev Adresi</label>
                        <textarea class="form-control" name="example-textarea-input" rows="6" placeholder="Ev Adresi giriniz."></textarea>
                    </div>

                    <div class="col-12 col-md-6">
                        <h2 style="color:green; text-align:center;">Öğrenim Durumu ve Mesleki Bilgiler</h2>
                    </div>

                    <div class="col-12 col-md-6 mb-3 d-flex flex-row gap-1">
                        <div class="col-6 d-flex flex-column justify-content-between">
                            <label class="form-label text-center">Öğrenim Durumunuz</label>
                            <select class="">
                                <option value="İlkokul">İlkokul</option>
                                <option value="Ortaokul">Ortaokul</option>
                                <option value="Lise">Lise</option>
                                <option value="Yüksek Okul">Yüksek  Okul</option>
                                <option value="Üniversite">Üniversite</option>
                                <option value="Diğer">Diğer</option>
                            </select>
                        </div>
                        <div class="col-6 d-flex flex-column justify-content-between">
                            <label class="form-label text-center">Pozisyonlar</label>
                            <select class="">
                                <option value="Muhasebe">Muhasebe</option>
                                <option value="Yazılım">Yazılım</option>
                                <option value="Operasyon">Operasyon</option>
                                <option value="İstihbarat">İstihbarat</option>
                            </select>
                        </div>
                    </div>

                    <div class="col-12 col-md-6 mb-3">
                        <label class="form-label text-center">Beklediğiniz Maaş</label>
                        <input type="number" class="form-control" name="example-text-input" placeholder="Beklediğiniz Maaşı Giriniz.">
                    </div>

                    <div class="col-12 col-md-6 mb-3">
                        <label class="form-label text-center">Seçenekleri Size Uygun Şekilde Doldurunuz.</label>

                        <label class="form-check form-switch">
                            <input class="form-check-input" type="checkbox">
                            <span class="form-check-label">Seyahat engeliniz var mı?</span>
                        </label>

                        <label class="form-check form-switch">
                            <input class="form-check-input" type="checkbox">
                            <span class="form-check-label">Fazla mesai yapmanızın sakıncası var mı?</span>
                        </label>

                        <label class="form-check form-switch">
                            <input class="form-check-input" type="checkbox">
                            <span class="form-check-label">Firmamızda çalışan tanıdığınız var mı?</span>
                        </label>

                        <label class="form-check form-switch">
                            <input class="form-check-input" type="checkbox">
                            <span class="form-check-label">Firmamıza daha önce başvurdunuz mu?</span>
                        </label>

                        <label class="form-check form-switch">
                            <input class="form-check-input" type="checkbox">
                            <span class="form-check-label">Herhangi bir sakatlığınız var mı?</span>
                        </label>

                        <label class="form-check form-switch">
                            <input class="form-check-input" type="checkbox">
                            <span class="form-check-label">Herhangi bir sabıka kaydınız var mı?</span>
                        </label>
                    </div>

                    <div class="col-12 col-md-6">
                        <h2 style="color:green; text-align:center;">
                            İş Deneyimleriniz
                        </h2>
                    </div>

                    <div class="col-12 col-md-6">
                        <h2 style="color:green; text-align:center;">
                            Referanslarınız
                        </h2>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>


